import java.util.Scanner;

class Practical6 {

	public static void main(String[] args) {		Scanner sc = new Scanner(System.in);

		System.out.print("Enter Number: ");

		int x=sc.nextInt();

		

		int i=2;

		for(;i<x;i++)

		{

			if (x%i==0)

			break;

		}

		

		if (x==i)

		System.out.print("\nPrime Number");

		

		else

		System.out.print("\nNot a Prime Number");

	}

}
