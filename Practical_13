class Demo  

{  

	void display()  //non-static function   	{  

		System.out.println("A non-static function is called.");  

	}  

	static void show()  //static function  

	{  

		System.out.println("\nThe static function is called.");      

	}

} 

	

public class prac13  

{  

	public static void main(String args[])  

	{  

		Demo obj = new Demo();  

		obj.display();  

		Demo.show();  

	}  

}  
